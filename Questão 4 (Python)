sp = 67836.43
rj = 36678.66
mg = 29229.88
es = 27165.48
ot = 19849.53
t = sp + rj + mg + es + ot

psp = (sp/t)*100
prj = (rj/t)*100
pmg = (mg/t)*100
pes = (es/t)*100
pot = (ot/t)*100

print("Sao Paulo", psp, "%")
print("Rio de Janeiro", prj, "%")
print("Minas Gerais", pmg, "%")
print("Espirito Santo", pes, "%")
print("Outros Estados", pot, "%")
